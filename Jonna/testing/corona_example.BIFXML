<?xml version="1.0" encoding="US-ASCII"?>

<!--Author: Andreas Sauter
    Bayesian network in XMLBIF v0.3 (BayesNet Interchange Format)
 -->

<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
	      <!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
	      <!ATTLIST VARIABLE TYPE (nature|decision|utility) "nature">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>

<BIF VERSION="0.3">
<NETWORK>
    <NAME>Corona Example</NAME>

    <!-- Variables-->
    <VARIABLE TYPE="nature">
        <NAME>Winter?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Senior?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Reproduction Number Below One?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Lockdown?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Went On Vacation?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Obesitas?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Disease?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Corona?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Symptoms?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Positive Selftest?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Adhere To Measures?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Contact With Infected Person?</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>

    <!-- PCTs -->
    <DEFINITION>
        <FOR>Winter?</FOR>
        <TABLE>0.25 0.75</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Reproduction Number Below One?</FOR>
        <TABLE>0.1 0.9</TABLE>
    </DEFINITION>
     <DEFINITION>
        <FOR>Senior?</FOR>
        <TABLE>0.198 0.802</TABLE>
    </DEFINITION>


    <DEFINITION>
        <FOR>Lockdown?</FOR>
        <GIVEN>Winter?</GIVEN>
        <GIVEN>Reproduction Number Below One?</GIVEN>
        <TABLE>0.5 0.5 0.8 0.2 0.1 0.9 0.6 0.4</TABLE> <!-- Order? Winter?, Reproduction Number Below One?, Lockdown?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Adhere To Measures?</FOR>
        <GIVEN>Senior?</GIVEN>
        <GIVEN>Lockdown?</GIVEN>
        <TABLE>0.65 0.35 0.40 0.60 0.10 0.90 0.05 0.95</TABLE> <!-- Order? Senior?, Lockdown?, Adhere To Measures?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Went On Vacation?</FOR>
        <GIVEN>Winter?</GIVEN>
        <GIVEN>Lockdown?</GIVEN>
        <GIVEN>Adhere To Measures?</GIVEN>
        <TABLE>0.15 0.85 0.90 0.10 0.80 0.20 0.90 0.10 0.60 0.40 0.90 0.10 0.85 0.15 0.95 0.05</TABLE> <!-- Order? Winter?, Lockdown?, Adhere To Measures?, Went On Vacation?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Obesitas?</FOR>
        <GIVEN>Senior?</GIVEN>
        <GIVEN>Lockdown?</GIVEN>
        <TABLE>0.90 0.10 0.55 0.45 0.25 0.75 0.15 0.85</TABLE> <!-- Order? Senior?, Lockdown?, Obesitas?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Disease?</FOR>
        <GIVEN>Senior?</GIVEN>
        <GIVEN>Obesitas?</GIVEN>
        <TABLE>0.75 0.25 0.25 0.75 0.20 0.80 0.05 0.95</TABLE> <!-- Order? Senior?, Obesitas?, Disease?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Contact With Infected Person?</FOR>
        <GIVEN>Adhere To Measures?</GIVEN>
        <GIVEN>Went On Vacation?</GIVEN>
        <TABLE>0.35 0.65 0.15 0.85 0.90 0.10 0.40 0.60</TABLE> <!-- Order? Adhere To Measures?, Went On Vacation?, Contact With Infected Person?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Symptoms?</FOR>
        <GIVEN>Went On Vacation?</GIVEN>
        <GIVEN>Contact With Infected Person?</GIVEN>
        <TABLE>0.95 0.05 0.10 0.90 0.85 0.15 0.01 0.99</TABLE> <!-- Order? Went On Vacation?, Contact With Infected Person?, Symptoms?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Positive Selftest?</FOR>
        <GIVEN>Contact With Infected Person?</GIVEN>
        <GIVEN>Symptoms?</GIVEN>
        <TABLE>0.90 0.10 0.05 0.95 0.10 0.90 0.01 0.99</TABLE> <!-- Order? Contact With Infected Person?, Symptoms?, Positive Selftest?-->
    </DEFINITION>

    <DEFINITION>
        <FOR>Corona?</FOR>
        <GIVEN>Disease?</GIVEN>
        <GIVEN>Contact With Infected Person?</GIVEN>
        <GIVEN>Symptoms?</GIVEN>
        <GIVEN>Positive Selftest?</GIVEN>
        <TABLE>0.90 0.10 0.35 0.65 0.30 0.70 0.25 0.75 0.20 0.80 0.15 0.85 0.10 0.90 0.05 0.95 0.45 0.55 0.30 0.70 0.25 0.75 0.20 0.80 0.15 0.85 0.10 0.90 0.05 0.95 0.0 1.0</TABLE> <!-- Order? Disease?, Contact With Infected Person?, Symptoms?, Positive Selftest?, Corona?-->
    </DEFINITION>


</NETWORK>
</BIF>